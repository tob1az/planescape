#create-pot:
#	for file in $(TRAFILES); do \
#          $(DLG2POT) -i orig/$$file -o pot/$$file.pot; \
#	done

#create-po:
#	for file in $(TRAFILES); do \
#	  if test ! -e pot/$$file.pot; then \
#	  	$(MAKE) create-pot; \
#	  fi; \

create-po:
	if test ! -e po/$$file.po; then \
	  	echo $(MSGINIT) -i pot/$$file.pot -o po/$$file.po --no-translator -l ru_RU.UTF-8; \
	  	$(MSGINIT) -i pot/$$file.pot -o po/$$file.po --no-translator -l ru_RU.UTF-8; \
	  fi; \
	done


update-po:
	for file in $(TRAFILES); do \
		if test ! -e po/$$file.po; then \
	  		$(MAKE) create-po; \
		fi; \
		$(MSGMERGE) -U po/$$file.po pot/$$file.pot; \
	done

create-tra:
	$(PO2DLG) -d orig/$(ALL) -p po/$(ALL).po -o trans/$(ALL)

#use-compendium:
#	for file in $(TRAFILES); do \
#		echo $(MSGMERGE) -U -C all/compendium.po po/$$file.po pot/$$file.pot; \
#		$(MSGMERGE) -U -C all/compendium.po po/$$file.po pot/$$file.pot; \
#	done

